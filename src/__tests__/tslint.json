{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
		"max-classes-per-file":false,
		"curly": false,
      "interface-name": false,
		"object-literal-sort-keys": false,
      "ordered-imports": [
	     true,
	     {
		 "named-imports-order": "any"
		  }
		],
		"no-bitwise": false,
	}
}
